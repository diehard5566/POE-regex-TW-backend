{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/maps",
        "dest": "/api/v1/maps"
      },
      {
        "src": "/(.*)",
        "dest": "/api/v1/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ]
  }